{
  "expo": {
    "name": "지금 방탈출",
    "slug": "now_escape",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./asset/icon_app_1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_demo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "now_escape",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "now_escape",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./asset/icon_app_1024.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
